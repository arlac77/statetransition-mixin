{
  "name": "statetransition-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare state transition methods like start & stop",
  "main": "dist/StateTransitionMixin.js",
  "module": "src/StateTransitionMixin.js",
  "scripts": {
    "cover": "nyc npm test",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/StateTransitionMixin.js -c -- src/StateTransitionMixin.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/statetransition-mixin.git"
  },
  "engines": {
    "node": ">=8.1.4"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "cracks": "3.1.2",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rollup": "^0.45.1",
    "semantic-release": "^6.3.6"
  },
  "keywords": [
    "state",
    "mixin"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/statetransition-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/statetransition-mixin#readme",
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}