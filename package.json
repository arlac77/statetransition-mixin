{
  "name": "statetransition-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare state transition methods like start & stop",
  "main": "dist/StateTransitionMixin.js",
  "jsnext:main": "src/StateTransitionMixin.js",
  "module": "src/StateTransitionMixin.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "docs": "jsdoc2md -l off -t README.hbs -f src/StateTransitionMixin.js > README.md",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/StateTransitionMixin.js -- src/StateTransitionMixin.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/statetransition-mixin.git"
  },
  "engines": {
    "node": ">=6.5"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "1.2.0",
    "istanbul": "0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.8.1",
    "mocha": "3.1.2",
    "rollup": "^0.36.1",
    "semantic-release": "^6.3.1"
  },
  "keywords": [
    "state",
    "mixin"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/statetransition-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/statetransition-mixin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
